
@{
    ViewBag.Title = "Interests";
}


    
    

@{
    
    System.Collections.ArrayList Flist = (System.Collections.ArrayList) ViewData["FBFriends"];
    
    if ( Flist != null )
    {
         int count = Flist.Count;
        
        

        for (int i = 0; i < count; i++)
        {
            dynamic f = Flist[i];

            string f2 = f.Name + "<br>";
        
            @Html.Raw(f2)

        }
        
       
    }

    
   
    }
    

